<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title><%= projectName %></title>
    <parser>
        <default-package-name><%= projectName %></default-package-name>
        <target>docs</target>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <files>
        <directory>src</directory>
    </files>
    <transformations>
        <template name="clean" />
    </transformations>
</phpdoc>
