{
    "source": {
        "include": [ "lib", "config", "index.js" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "public/minami",
        "destination": "./docs/",
        "encoding": "utf8"
    },
    "plugins": [
        "plugins/markdown"
    ]
}
